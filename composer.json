{
    "name": "wyrihaximus/metrics-tactician",
    "description": "Metric collection for Tactician",
    "license": "MIT",
    "authors": [
        {
            "name": "Cees-Jan Kiewiet",
            "email": "ceesjank@gmail.com"
        }
    ],
    "require": {
        "php": "^8 || ^7.4",
        "league/tactician": "^1.1.0",
        "wyrihaximus/metrics": "^3 || ^2 || ^1.1"
    },
    "require-dev": {
        "phpbench/phpbench": "^0.17.1 || ^1.0.0",
        "thecodingmachine/safe": "^1.3.3",
        "wyrihaximus/test-utilities": "^3.7.4 || ^7.0.0"
    },
    "autoload": {
        "psr-4": {
            "WyriHaximus\\Metrics\\Tactician\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WyriHaximus\\Tests\\Metrics\\Tactician\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "infection/extension-installer": true,
            "ergebnis/composer-normalize": true,
            "icanhazstring/composer-unused": true
        },
        "platform": {
            "php": "7.4.7"
        },
        "sort-packages": true
    },
    "scripts": {
        "post-install-cmd": [
            "composer normalize"
        ],
        "post-update-cmd": [
            "composer normalize"
        ]
    }
}
